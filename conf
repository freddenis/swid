- config:        # format is "config:value"
	 begin: mailx -s "done" people@company.company
	   end: echo "done"
	during: mailx -s $STEP done people@company.company
	 email: people@company.com
- names:         # format is "alias:command_to_execute"
	backup: backup.sh
	step1 : launch_sql.sh step1
	step2: launch_sql.sh step2		# a comment
# step 3: dksdjs			# not this one
		final_step:final_step.sh -p blabla
- dependencies:   # format is "alias:dependency1 dependency2"
        backup
 step1 : backup
   step2 : backup			# a comment
final_step : step1 step2
